@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

@{ var home = CurrentPage.Site().Children.Where("Visible"); }
  <ul>
@if (home.Any())
{		 		          
        foreach (var item in home)
        {
			if (item.Name.ToUpper() != "SEARCH")
			{
            	<li><a href="@item.Url">@item.Name</a> &raquo;</li>				
        	}

		}
}

 </ul>